    "mochify": "^2.6.1",
    "phantomjs": "^1.9.15",
    "power-assert": "^0.10.1",
    "uglify-js": "^2.4.17"
  },
  "dependencies": {
    "browserify-shim": "^3.8.2",
    "jquery": "^2.1.3",
    "throttle-debounce": "^0.1.0"
